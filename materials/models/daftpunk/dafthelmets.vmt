"VertexLitGeneric"
{
	"$baseTexture" "models\daftpunk\DaftHelms_animation"
	"$bumpmap" "models\daftpunk\DaftHelmets_n"
	"$envmap" "env_cubemap"
	"$envmapcontrast" "0.8"  
	"$envmapsaturation" "[.5 .4 1.3]"
	"$envmaptint" "[.6 .6 .6]"
 	"Proxies"
	{
      	"AnimatedTexture"
      	{
        	 "animatedtexturevar" "$basetexture"
