"UnlitGeneric"
{
	"$basetexture"	"scope/sfscope_spinner"
	"$color"		"{81 202 205}"
	"$translucent"	"1"
	"$additive"		"1"
	"$ignorez"		"1"

	"$angle" 0.0
	"$translate" "[0.0 0.0]"
	"$center" "[0.5 0.5]"

	"Proxies"
	{
		"LinearRamp"
		{
			"rate" 180
			"initialValue" 0.0
			"resultVar" "$angle"
		}

		"TextureTransform"
		{
			"translateVar" "$translate"
			"rotateVar" "$angle"
			"centerVar" "$center"
			"resultVar" "$basetexturetransform"
		}
	}
}